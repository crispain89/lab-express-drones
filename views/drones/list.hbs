<h2>Available drones</h2>
 {{#each drones}}

    <div class="drones">
        <span>{{this.name}}</span>
        <span>{{this.propellers}}</span>
        <span>{{this.maxSpeed}}</span>
        <a href="/drones/{{this.id}}/edit">modificar dron </a>
        <form action="/drones/{{this.id}}/delete" method="POST">
            <button href="/drones/{{this.id}}/delete">ELIMIAR dron </button>
        </form>

    </div>
 {{/each}}